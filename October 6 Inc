first_name = "Saket"
last_name = "Waghmode" 
age = 42
format_example = "Hi, my name is Saket Waghmode. I am 42 years old."
age = 42 

print(format_example.format(first_name,last_name,age))
print(age.isdecimal())
print(first_name.isalpha())
print(age== "42")
print(age== 42)

